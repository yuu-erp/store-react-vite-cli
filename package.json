{
  "name": "webpack-cli",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start:test-app": "cd apps/test-app && concurrently \"yarn dev --port 8080\"",
    "start:test-app-dev": "concurrently \"yarn tailwind\" \"yarn start:test-app\"",
    "build:test-app": "cd apps/test-app && concurrently \"yarn build\"",
    "tailwind": "postcss packages/styles/tailwind.css -o packages/styles/global.css --watch"
  },
  "dependencies": {
    "@tanstack/router-vite-plugin": "^1.8.2",
    "postcss-cli": "^11.0.0",
    "postcss-comment": "^2.0.0",
    "postcss-custom-properties-fallback": "^1.0.2",
    "postcss-import": "^16.0.0",
    "postcss-preset-env": "^9.3.0",
    "postcss-scss": "^4.0.9",
    "postcss-simple-vars": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
